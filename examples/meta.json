{
	"description":"Used to parse future grammars",
	"symbols":{
		"atom_lit": { 
			"terminal":true,
			"match":"[a-zA-Z_\\+\\*\\(\\)]([^ \t\n\\.]*)?"
		},
		"atom_quote": {
			"terminal":true,
			"match":"[\\\"](.*)[\\\"]"
		},
		"atom_single_quote": {
			"terminal":true,
			"match":"[\\'](.*)[\\']"
		},
		"->": { 
			"terminal":true, 
			"excludeFromProduction":true,
			"match":"->"
		},
		".": { 
			"terminal":true, 
			"excludeFromProduction":true,
			"match":"\\."
		},
		"|": { 
			"terminal":true, 
			"excludeFromProduction":true,
			"match":"\\|"
		},
		"WS": { 
			"includeInStream":false,
			"terminal":true,
			"excludeFromProduction":true,
			"match":"[ \t\n]+"
		}
	},
	"productions":{
		"Grammar":[
			["GrammarLine", "Grammar"],
			["GrammarLine"]
		],
		"GrammarLine":[
			["atom", "->", "SymbolList", "."]
		],
		"SymbolList":[
			["AtomList", "|", "SymbolList"],
			["AtomList"]
		],
		"AtomList":[
			[ "atom", "AtomList" ],
			[ "atom" ]
		],
		"atom":[
			["atom_quote"],
			["atom_lit"],
			["atom_single_quote"]

		]
	},
	"startSymbols": [ "Grammar" ]
}